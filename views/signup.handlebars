<div class="translucent-form-overlay">
        <form class="signup-form">
          <h3>Signup</h3>
          <div class="row columns">
            <label for ="username-signup">Username</label>
              <input type="text" id="username-signup" placeholder="Any"/>
          </div>
          <div class="row columns">
            <label for ="email-signup">Email</label>
            <input type="text" id="email-signup" placeholder="Any"/>
            </label>
          </div>
          <div class="row columns">
            <label for ="password-signup">Password</label>
              <input type="password" id="password-signup" placeholder="Any"/>
            
          </div>
          
          <button type="submit" class="primary button expanded search-button">
            Sign up
          </button>
       </form>
       <a href="/login">Login</a>
      </div>
      <script src="/javascript/signup.js"></script>